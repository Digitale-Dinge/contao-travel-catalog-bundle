    "autoload": {
        "psr-4": {
            "DigitaleDinge\\TravelCatalogBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "DigitaleDinge\\TravelCatalogBundle\\DigitaleDingeTravelCatalogBundle"
